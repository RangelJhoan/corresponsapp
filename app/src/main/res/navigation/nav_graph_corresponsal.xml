        <action
            android:id="@+id/action_depositarFragment_to_corresponsalMenuFragment"
            app:destination="@id/corresponsalMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/transferirFragment"
        android:name="com.example.corresponsapp.interfacesgraficas.corresponsal.TransferirFragment"
        android:label="fragment_transferir"
        tools:layout="@layout/fragment_transferir" >
        <action
            android:id="@+id/action_transferirFragment_to_corresponsalMenuFragment"
            app:destination="@id/corresponsalMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/crearCuentaFragment"
        android:name="com.example.corresponsapp.interfacesgraficas.corresponsal.crearcuenta.CrearCuentaFragment"
