<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_corresponsal"
    app:startDestination="@id/corresponsalMenuFragment">

    <fragment
        android:id="@+id/corresponsalMenuFragment"
        android:name="com.example.corresponsapp.interfacesgraficas.corresponsal.corresponsalmenu.CorresponsalMenuFragment"
        android:label="fragment_corresponsal_menu"
        tools:layout="@layout/fragment_corresponsal_menu" >
        <action
            android:id="@+id/action_corresponsalMenuFragment_to_retirarFragment"
            app:destination="@id/retirarFragment" />
        <action
            android:id="@+id/action_corresponsalMenuFragment_to_depositarFragment"
            app:destination="@id/depositarFragment" />
        <action
            android:id="@+id/action_corresponsalMenuFragment_to_transferirFragment"
            app:destination="@id/transferirFragment" />
        <action
            android:id="@+id/action_corresponsalMenuFragment_to_crearCuentaFragment"
            app:destination="@id/crearCuentaFragment" />
        <action
            android:id="@+id/action_corresponsalMenuFragment_to_pagoTarjetaFragment"
            app:destination="@id/pagoTarjetaFragment" />
    </fragment>
    <fragment
        android:id="@+id/retirarFragment"
        android:name="com.example.corresponsapp.interfacesgraficas.corresponsal.retirar.RetirarFragment"
        android:label="fragment_retirar"
