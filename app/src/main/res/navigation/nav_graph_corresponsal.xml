        <action
            android:id="@+id/action_retirarFragment_to_corresponsalMenuFragment"
            app:destination="@id/corresponsalMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/depositarFragment"
        android:name="com.example.corresponsapp.interfacesgraficas.corresponsal.depositar.DepositarFragment"
